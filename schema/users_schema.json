{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "address": {
      "street": {"type": "string"},
      "suite": {"type": "string"},
      "city": {"type": "string"},
      "zipcode": {"type": "string"},
      "geo": {
        "lat": {"type": "string"},
        "long": {"type": "string"}
      }
    },
    "phone": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "company": {
      "name": {"type": "string"},
      "catchPhrase": {"type": "string"},
      "bs": {"type": "string"}
    }
  },
  "required": [
    "id",
    "name",
    "username",
    "email",
    "address",
    "phone",
    "website",
    "company"
  ]
}
